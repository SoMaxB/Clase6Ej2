class Alumno ():
    def __init__ (self, nombre, nota):
        self.nombre = nombre
        self.nota = nota

    def notificar (self):
        print ("Nombre", self.nombre)
        print ("Nota", self.nota)

    def notas (self):
        if self.nota >= 6:
            print (self.nombre, "Aprobado")
        else:
            print (self.nombre, "Suspenso")


if __name__ == "__main__":
    Alumno1 = Alumno("Juan", 8)
    Alumno1.notificar()
    Alumno1.notas()
